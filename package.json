{
  "dependencies": {
    "mobx": "^5.9.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "asciitree": "^1.0.2",
    "jest": "^24.3.1",
    "parcel-bundler": "^1.12.0",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  },
  "browserslist": [
    "> 5%"
  ],
  "scripts": {
    "test": "node_modules/.bin/jest",
    "browser": "node_modules/.bin/parcel dev/index.html --no-hmr"
  },
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "\\.util\\.ts$",
      "\\.ignore\\.ts$"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  }
}
